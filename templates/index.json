      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "custom_liquid": "<style>\n  .hero-header-container {\n    background-color: #0063d9; /* Your brand's primary blue */\n    color: #ffffff;\n    text-align: center;\n    padding: 80px 20px 20px 20px; /* Generous padding for desktop */\n    line-height: 1.4; /* Slightly increased for better readability across lines */\n  }\n\n  .hero-line-1 {\n    font-family: 'Abel', sans-serif;\n    font-size: 60px;\n    font-weight: 400;\n    margin: 0 0 35px 0; /* Increased space below */\n    letter-spacing: 1px;\n    text-transform: uppercase; /* All caps */\n    color: #ffffff !important; /* Force white text */\n  }\n\n  .hero-line-2 {\n    font-family: 'Abel', sans-serif;\n    font-size: 30px;\n    font-weight: 700;\n    margin: 0 0 25px 0;\n   \n   letter-spacing: 1.5px;\n    color: #ffffff !important; /* Force white text */\n  }\n\n  .hero-line-3 {\n    font-family: 'Abel', sans-serif;\n    font-size: 22px;\n    font-weight: 400;\n    margin: 0 0 10px 0;\n    letter-spacing: 0.5px;\n    color: #ffffff !important; /* Force white text */\n  }\n\n  .hero-line-4 {\n    font-family: 'Abel', sans-serif;\n    font-size: 20px;\n    font-weight: 400;\n    margin: 0;\n    opacity: 0.9;\n    color: #ffffff !important; /* Force white text */\n  }\n\n  /* Responsive adjustments for mobile */\n  @media (max-width: 767px) {\n    .hero-header-container {\n      padding: 50px 15px 40px 15px; /* Reduced and adjusted padding: more top space, less bottom, tighter sides for mobile */\n      line-height: 1.3; /* Slightly tighter line height on mobile to save vertical space */\n    }\n    \n    .hero-line-1 {\n      font-size: 42px; /* Further scaled down from 48px for better mobile fit without overflow */\n      margin: 0 0 20px 0; /* Reduced bottom margin to prevent excessive vertical spacing */\n      letter-spacing: 0.8px; /* Slightly reduced for mobile readability */\n    }\n    \n    .hero-line-2 {\n      font-size: 20px !important;\n      margin: 0 0 15px 0; /* Reduced bottom margin */\n      letter-spacing: 1.2px; /* Slightly reduced */\n    }\n    \n    .hero-line-3 {\n      font-size: 16px; /* Slightly reduced from 18px for tighter fit */\n      margin: 0 0 8px 0; /* Reduced bottom margin */\n      letter-spacing: 0.4px;\n    }\n    \n    .hero-line-4 {\n      font-size: 15px; /* Slightly reduced from 16px */\n      letter-spacing: 0.3px;\n    }\n  }\n\n  /* Additional fine-tuning for very small mobile screens (e.g., iPhone SE) */\n  @media (max-width: 480px) {\n    .hero-header-container {\n      padding: 40px 10px 30px 10px; /* Even tighter padding for smallest screens */\n    }\n    \n    .hero-line-1 {\n      font-size: 36px;\n      margin: 0 0 15px 0;\n    }\n    \n    .hero-line-2 {\n      font-size: 18px !important;\n      margin: 0 0 12px 0;\n    }\n    \n    .hero-line-3 {\n      font-size: 15px;\n      margin: 0 0 5px 0;\n    }\n    \n    .hero-line-4 {\n      font-size: 14px;\n    }\n  }\n</style>\n\n<div class=\"hero-header-container\">\n  <h1 class=\"hero-line-1\">Points, Panels & Accessories </h1>\n</div>"
      }
